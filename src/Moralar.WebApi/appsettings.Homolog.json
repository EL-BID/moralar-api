{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "EnableSwagger": true,
  "EnableService": true,

  "DATABASE": {
    "CONNECTIONSTRING": "mongodb+srv://{user-db: default: sa}:{contraseña de la bd}@{host de mongodb}?retryWrites=true&w=majority&appName={app-name del cluster}",
    "NAME": "moralardb",
    "User": "{user-db: default: sa}",
    "Password": "{contraseña de la bd}",
    "Remote": "{host de mongodb}",
    "Local": "{host de mongodb}"
  },
  "Config": {
    "BaseUrl": "https://{api-url}/content/upload/",
    "DefaultUrl": "https://{api-url}/content/images/default.png",
    "CustomUrls": [
      "https://{api-url}/"
    ]
  },
  "Language": "pt-BR",
  "Jwt": {
    "ProjectFirebaseId": "",
    "Issuer": "Moralar",
    "Audience": "Moralar",
    "SecretKey": "{secret-key}",
    "TokenFrom": "fromhours",
    "TokenValue": 1
  }
}